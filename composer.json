{
    "name": "kuria/dom",
    "description": "Wrappers around the PHP DOM classes",
    "keywords": ["DOM", "document object model", "wrapper", "DOMDocument", "XPath", "HTML", "XML", "fragment"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "ext-dom": "*",
        "ext-libxml": "*",
        "kuria/simple-html-parser": "^2.0"
    },
    "require-dev": {
        "kuria/dev-meta": "^0.4.0"
    },
    "autoload": {
        "psr-4": {
            "Kuria\\Dom\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kuria\\Dom\\": "tests"
        },
        "files": [
            "tests/Mocks/Tidy.mock"
        ]
    }
}